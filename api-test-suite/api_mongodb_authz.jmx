        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Connect Mongo &amp; Insert &quot;mqtt_acl&quot;" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.*
import com.gmongo.GMongoClient
import com.mongodb.MongoCredential
import com.mongodb.ServerAddress

// disable user and pwd connect
GMongoClient client = new GMongoClient(new ServerAddress(&quot;${mongodb_ip}&quot;,27017))

def DB=client.getDB(&quot;mqtt&quot;)
//insert data
def dataDB=DB.getCollection(&quot;mqtt_acl&quot;).insert([&quot;username&quot; : &quot;test&quot;, &quot;clientid&quot; : &quot;test&quot;, &quot;ipaddress&quot; : &quot;127.0.0.1&quot;, &quot;permission&quot;: &quot;allow&quot;,&quot;action&quot;:&quot;all&quot;,&quot;topics&quot;:[&quot;t/1&quot;]])
String ReData=dataDB

SampleResult.setResponseData(ReData)</stringProp>
          <stringProp name="TestPlan.comments">disable account password connection</stringProp>
        </JSR223Sampler>
